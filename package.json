{
    "name": "travi-node-backend",
    "version": "1.0.0",
    "description": "Node.js REST Api server",
    "main": "server.js",
    "scripts": {
        "start": "nodemon server.js --exec babel-node --presets env",
        "release": "npm run clean && npm run build && npm run serve",
        "clean": "rm -rf dist && mkdir dist",
        "build": "babel . -s -D -d dist --presets env --ignore node_modules",
        "serve": "node dist/server.js",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [
        "example",
        "heroku"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Travipraveen/travi-node-backend.git"
    },
    "author": "Praveen S",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Travipraveen/travi-node-backend/issues"
    },
    "homepage": "https://github.com/Travipraveen/travi-node-backend#readme",
    "dependencies": {
        "body-parser": "^1.19.0",
        "express": "^4.17.1",
        "mongoose": "^5.8.3"
    },
    "devDependencies": {
        "@babel/core": "^7.7.7",
        "@babel/node": "^7.7.7",
        "@babel/preset-env": "^7.7.7",
        "@babel/register": "^7.7.7",
        "babel-cli": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "nodemon": "^2.0.2"
    },
    "engines": {
        "node": "10.x"
    }
}
